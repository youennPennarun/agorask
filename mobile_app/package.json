{
  "name": "agorask",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node_modules/react-native/packager/packager.sh",
    "test": "jest",
    "flow": "flow",
    "codecov": "jest && codecov",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "release": "ENVFILE=.env.production cd android && ./gradlew assembleRelease",
    "buildDev": "ENVFILE=.env.dev cd android && ./gradlew assembleDevRelease"
  },
  "dependencies": {
    "apollo-client": "0.10.1",
    "graphql-tag": "^1.2.1",
    "immutability-helper": "^2.1.1",
    "moment": "2.17.1",
    "ngeohash": "^0.6.0",
    "react": "^15.4.1",
    "react-apollo": "0.13.2",
    "react-native": "^0.39.0",
    "react-native-config": "0.3.1",
    "react-native-image-picker": "0.24",
    "react-native-material-kit": "git://github.com/youennPennarun/react-native-material-kit#cc9ac25",
    "react-native-vector-icons": "^4.0.0",
    "react-redux": "^5.0.1",
    "redux": "^3.6.0",
    "redux-logger": "2.7.4",
    "redux-persist": "^4.4.2",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel-jest": "^18.0.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-react-native": "^1.9.1",
    "codecov": "^1.0.1",
    "enzyme": "^2.7.0",
    "enzyme-to-json": "^1.4.5",
    "eslint": "3.17.1",
    "eslint-config-airbnb-base": "11.1.1",
    "eslint-plugin-import": "2.2.0",
    "flow-bin": "0.41.0",
    "jest": "^18.1.0",
    "react-dom": "15.4.1",
    "react-native-mock": "^0.2.7",
    "reactotron-react-native": "^1.6.0",
    "reactotron-redux": "^1.6.1"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "./jest/setup.js"
    ],
    "automock": false,
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleFileExtensions": [
      "js",
      "ios.js",
      "android.js"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  }
}
