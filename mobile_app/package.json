{
  "name": "agorask",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node_modules/react-native/packager/packager.sh",
    "test": "jest",
    "flow": "flow",
    "codecov": "jest && codecov",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "release": "ENVFILE=.env.production cd android && ./gradlew assembleRelease",
    "buildDev": "ENVFILE=.env.dev cd android && ./gradlew assembleDevRelease"
  },
  "dependencies": {
    "apollo-client": "^0.5.18",
    "graphql-tag": "^1.1.2",
    "immutability-helper": "^2.0.0",
    "moment": "2.17.1",
    "react": "^15.4.1",
    "react-apollo": "^0.7.1",
    "react-native": "^0.39.0",
    "react-native-code-push": "^1.16.0-beta",
    "react-native-config": "^0.1.2",
    "react-native-material-kit": "git://github.com/youennPennarun/react-native-material-kit#cc9ac25",
    "react-native-svg": "^4.3.1",
    "react-native-vector-icons": "3.0.0",
    "react-redux": "4.4.6",
    "redux": "^3.6.0",
    "redux-logger": "2.7.4",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel-jest": "17.0.2",
    "babel-polyfill": "^6.20.0",
    "babel-preset-react-native": "1.9.0",
    "codecov": "^1.0.1",
    "enzyme": "2.6.0",
    "enzyme-to-json": "1.3.0",
    "eslint": "3.11.1",
    "eslint-config-airbnb-base": "10.0.1",
    "eslint-plugin-import": "2.2.0",
    "flow-bin": "0.36.0",
    "jest": "17.0.3",
    "jest-react-native": "17.1.0",
    "react-addons-test-utils": "^15.4.1",
    "react-dom": "15.4.1",
    "react-native-mock": "^0.2.7",
    "react-test-renderer": "15.3.2",
    "reactotron-react-native": "1.5.3",
    "reactotron-redux": "1.5.3"
  },
  "jest": {
    "preset": "jest-react-native",
    "setupFiles": [
      "jest/setup.js"
    ],
    "automock": false,
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleFileExtensions": [
      "js",
      "ios.js",
      "android.js"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  }
}
